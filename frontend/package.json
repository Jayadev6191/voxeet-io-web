{
  "name": "voxeet-io",
  "version": "1.0.7",
  "private": true,
  "main": "src/main.js",
  "scripts": {
    "dist": "npm run build:electron && ELECTRON_BUILDER_ALLOW_UNRESOLVED_DEPENDENCIES=true build",
    "bootstrap": "yarn link @voxeet/voxeet-web-sdk && yarn link @voxeet/media-engine && yarn link @voxeet/react-components",
    "start": "webpack serve --hot --inline ",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js --progress",
    "builde": "cross-env NODE_ENV=development webpack --config webpack.config.electron.prod.js --progress",
    "build:electron": "cross-env DEBUG=*,-babel ELECTRON=true npm run builde",
    "get-version": "cross-env-shell echo ${npm_package_version}"
  },
  "author": "Voxeet",
  "contributors": [
    "MARTIN Valentin <vmartin@voxeet.com>",
    "GOURGUES Thomas <tgourgues@voxeet.com>"
  ],
  "browserslist": [
    "IE 11"
  ],
  "build": {
    "appId": "io.voxeet.app",
    "productName": "Voxeet",
    "extends": null,
    "files": [
      "dist/*.svg",
      "dist/*.ttf",
      "dist/sounds/*.mp3",
      "dist/index.html",
      "dist/bundle.js",
      "dist/images",
      "src/main.js",
      "src/preload.js",
      "node_modules/",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150
        }
      ]
    },
    "pkg": {
      "allowCurrentUserHome": false,
      "allowAnywhere": false
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "public/icon.png",
      "extraResources": [
        "libraries"
      ],
      "publish": {
        "provider": "s3",
        "bucket": "voxeet-io-electron-ci",
        "publishAutoUpdate": "true"
      }
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.png",
      "publish": {
        "provider": "s3",
        "bucket": "voxeet-io-electron-ci",
        "publishAutoUpdate": "true"
      }
    },
    "squirrelWindows": {
      "iconUrl": "https://raw.githubusercontent.com/voxeet/voxeet-io-web/master/public/icon.png"
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@trodi/electron-splashscreen": "^1.0.0",
    "@voxeet/react-components": "file:../../uxkit",
    "@voxeet/voxeet-web-sdk": "file:./../../voxeet-sdk/packages/voxeet-web-sdk/",
    "axios": "^0.21.1",
    "browserslist": "^4.16.6",
    "core-js": "^3.15.2",
    "express": "^4.17.1",
    "prettier": "^2.3.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-localization": "^1.0.17",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "resolutions": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "electron": "^9.2.0",
    "electron-builder": "^22.6.0",
    "electron-builder-squirrel-windows": "^22.6.1",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.1.0",
    "style-loader": "^3.2.1",
    "terser-webpack-plugin": "^5.1.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "peerDependencies": {
    "express": "^4.0.0"
  },
  "files": [
    "../README.md",
    "dist/"
  ]
}
